[
    {
      "selector": "node",
      "style": {
        "shape": "round-rectangle",
        "height": "8px",
        "width": "auto",
        "padding": "16px",
        "font-size": "14px",
        "text-valign": "center",
        "text-halign": "center",
        "background-color": "#f2f2f2",
        "border-width": "3",
        "border-color": "#5E5E5E",
        "box-radius": "5px"

      }
    },
    {
      "selector": "node[label]",
      "style": {
        "content": "data(label)"
      }
    },
    {
      "selector": ".package",
      "style": {
        "shape" : "rectangle",
        "text-valign": "top",
        "text-halign": "center",
        "padding": "16px",
        "border-width": "3px",
        "border-color": "#5E5E5E",
        "background-color": "#f2f2f2"
      }
    },
    {
      "selector": "node[hidden]",
      "style": {
        "display": "none"
      }
    },
    {
      "selector": "edge",
      "style": {
        "label": "data(label)",
        "curve-style": "bezier",
        "text-opacity": 1,
        "font-size": "10px",
        "color": "#000",
        "edge-text-rotation": "autorotate",
        "arrow-scale": 1,
        "min-zoomed-font-size": "6px"
      }
    }
  ]