[
    {
      "selector": "node",
      "style": {
        "shape": "round-rectangle",
        "min-width": "10px",
        "min-height": "8px",
        "height": "8px",
        "width": "label",
        "padding": "16px",
        "font-size": "14px",
        "content": "data(label)",
        "text-valign": "center",
        "text-halign": "center",
        "background-color": "#f2f2f2",
        "border-width": "1.5px",
        "border-color": "#5E5E5E",
        "box-radius": "5px"

      }
    },
    {
      "selector": ".package",
      "style": {
        "shape" : "rectangle",
        "text-valign": "top",
        "text-halign": "center",
        "padding": "16px",
        "border-width": "2px",
        "border-color": "#5E5E5E",
        "background-color": "#f2f2f2"
      }
    },
    {
      "selector": "node[hidden]",
      "style": {
        "display": "none"
      }
    },
    {
      "selector": "edge",
      "style": {
        "label": "data(label)",
        "curve-style": "bezier",
        "text-opacity": 1,
        "font-size": "10px",
        "color": "#000",
        "edge-text-rotation": "autorotate",
        "arrow-scale": 1,
        "min-zoomed-font-size": "8px",
        "width": "data(properties.weight)"
      }
    },
    {
      "selector": "edge[label = 'calls']",
      "style": {
        "line-fill": "linear-gradient",
        "line-gradient-stop-colors": ["#00203E", "#A8E8CA"]
      }
    },
    {
      "selector": "edge[label = 'accepts']",
      "style": {
        "line-fill": "linear-gradient",
        "line-gradient-stop-colors": ["#476B55", "#E9ED7B"]
      }
    },
    {
      "selector": "edge[label = 'holds']",
      "style": {
        "line-fill": "linear-gradient",
        "line-gradient-stop-colors": ["#AD2A2A", "#FFF200"]
      }
    },
    {
      "selector": "edge[label = 'accesses']",
      "style": {
        "line-fill": "linear-gradient",
        "line-gradient-stop-colors": ["#4D5282", "#FDFF75"]
      }
    },
        {
      "selector": "edge[label = 'specializes']",
      "style": {
        "line-fill": "linear-gradient",
        "line-gradient-stop-colors": ["#630063", "#00BBFF"]
      }
    },
    {
      "selector": "edge[label = 'constructs']",
      "style": {
        "line-fill": "linear-gradient",
        "line-gradient-stop-colors": ["#0066FF", "#FAB300"]
      }
    },
    {
      "selector": "edge[label = 'returns']",
      "style": {
        "line-fill": "linear-gradient",
        "line-gradient-stop-colors": ["#424242", "#FF008C"]
      }
    },
            {
      "selector": "edge[label = 'contains']",
      "style": {
        "line-fill": "linear-gradient",
        "line-gradient-stop-colors": ["#3B3B3B", "#9C9C9C"]
      }
    }
  ]